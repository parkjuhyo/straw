    "def file_read():\n",
    "    with open('friends.txt') as file:\n",
    "        while True:\n",
    "            line = file.readline()\n",
    "            if line == '':\n",
    "                break\n",
    "            yield line.strip('\\n')              \n",
    "for i in file_read():\n",
    "    print(i)"
   ]
  }
